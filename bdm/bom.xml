<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<businessObjectModel xmlns="http://documentation.bonitasoft.com/bdm-xml-schema/1.0" modelVersion="1.0" productVersion="7.15.0">
    <businessObjects>
        <businessObject qualifiedName="odonto_ufsc.Aluno">
            <fields>
                <field type="STRING" length="255" name="matricula" nullable="false" collection="false"/>
                <field type="STRING" length="255" name="disciplinas" nullable="true" collection="true"/>
                <field type="STRING" length="255" name="pacientesVinculados" nullable="true" collection="true"/>
                <field type="STRING" length="255" name="nome" nullable="false" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="odonto_ufsc.Endereco">
            <fields>
                <field type="STRING" length="255" name="endereco1" nullable="true" collection="false">
                    <description>Endereço: Rua, Avenida, etc</description>
                </field>
                <field type="STRING" length="255" name="endereco2" nullable="true" collection="false">
                    <description>Número da residência</description>
                </field>
                <field type="STRING" length="255" name="complemento" nullable="true" collection="false">
                    <description>Casa, apto, bloco, etc</description>
                </field>
                <field type="STRING" length="255" name="bairro" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="cidade" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="estado" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="pais" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="cep" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="telefoneFixo" nullable="true" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="odonto_ufsc.Paciente">
            <fields>
                <field type="STRING" length="255" name="cpf" nullable="false" collection="false">
                    <description>O CPF (Cadastro de Pessoa Física) é um número de identificação individual atribuído a cada cidadão brasileiro. Ele serve para identificar de forma única uma pessoa perante órgãos governamentais, empresas e instituições financeiras.</description>
                </field>
                <field type="STRING" length="255" name="nome" nullable="true" collection="false">
                    <description>O campo "Nome" se refere ao nome completo de uma pessoa&#13;
&#13;
</description>
                </field>
                <field type="STRING" length="255" name="telefone" nullable="false" collection="false">
                    <description>O campo "Telefone" é usado para armazenar o número de telefone de uma pessoa.&#13;
&#13;
</description>
                </field>
                <field type="BOOLEAN" length="255" name="telefoneTemWhatsApp" nullable="true" collection="false">
                    <description>Identifica se telefone é acessível para contato via WhatsApp.&#13;
&#13;
</description>
                </field>
                <field type="LOCALDATETIME" length="255" name="nascimento" nullable="true" collection="false">
                    <description>O campo "Data de Nascimento" guarda a data em que uma pessoa nasceu.&#13;
&#13;
</description>
                </field>
                <field type="STRING" length="255" name="cartaoSus" nullable="true" collection="false">
                    <description>O Cartão SUS é um documento utilizado no Brasil para acesso aos serviços públicos de saúde.&#13;
&#13;
</description>
                </field>
                <field type="STRING" length="255" name="raca" nullable="true" collection="false">
                    <description>O campo "Raça" é utilizado para registrar a identificação auto-declarada racial ou étnica de uma pessoa.&#13;
&#13;
</description>
                </field>
                <field type="STRING" length="255" name="sexo" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="genero" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="nacionalidade" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="horarioAtendimento" nullable="true" collection="true">
                    <description>Contem horarios de atendimento de preferencia do paciente.</description>
                </field>
                <field type="STRING" length="255" name="faltas" nullable="true" collection="false"/>
                <field type="STRING" length="255" name="situacao" nullable="true" collection="false">
                    <description>Identifica a situação do Paciente diante do tratamento. </description>
                </field>
                <relationField type="AGGREGATION" reference="odonto_ufsc.Endereco" fetchType="LAZY" name="endereco" nullable="true" collection="false">
                    <description>Informações de endereço do paciente.</description>
                </relationField>
                <relationField type="AGGREGATION" reference="odonto_ufsc.Prontuario" fetchType="LAZY" name="prontuario" nullable="true" collection="false">
                    <description>Prontuario do Paciente</description>
                </relationField>
                <relationField type="AGGREGATION" reference="odonto_ufsc.Aluno" fetchType="LAZY" name="alunoVinculado" nullable="true" collection="false">
                    <description>Aluno vinculado ao Paciente.</description>
                </relationField>
                <relationField type="AGGREGATION" reference="odonto_ufsc.QuadroDeNecessidades" fetchType="LAZY" name="quadro" nullable="true" collection="false">
                    <description>Quadro de Necessidades de atendimento do Paciente.</description>
                </relationField>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="odonto_ufsc.Prontuario">
            <fields>
                <field type="STRING" length="255" name="numero" nullable="true" collection="false"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
        <businessObject qualifiedName="odonto_ufsc.QuadroDeNecessidades">
            <fields>
                <field type="STRING" length="255" name="estomatologia" nullable="true" collection="true"/>
                <field type="STRING" length="255" name="periodontologia" nullable="true" collection="true"/>
                <field type="STRING" length="255" name="dentistica" nullable="true" collection="true"/>
                <field type="STRING" length="255" name="endodontia" nullable="true" collection="true"/>
                <field type="STRING" length="255" name="protese" nullable="true" collection="true"/>
            </fields>
            <uniqueConstraints/>
            <queries/>
            <indexes/>
        </businessObject>
    </businessObjects>
</businessObjectModel>
